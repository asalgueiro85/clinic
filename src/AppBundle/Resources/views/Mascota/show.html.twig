{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Mascota</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Sexo</th>
                <td>{{ entity.sexo }}</td>
            </tr>
            <tr>
                <th>Esterilizacion</th>
                <td>{{ entity.esterilizacion }}</td>
            </tr>
            <tr>
                <th>Fechanacimiento</th>
                <td>{{ entity.fechaNacimiento|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechaalta</th>
                <td>{{ entity.fechaAlta|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
            <tr>
                <th>Pedigri</th>
                <td>{{ entity.pedigri }}</td>
            </tr>
            <tr>
                <th>Numpedigri</th>
                <td>{{ entity.numPedigri }}</td>
            </tr>
            <tr>
                <th>Chip</th>
                <td>{{ entity.chip }}</td>
            </tr>
            <tr>
                <th>Caracter</th>
                <td>{{ entity.caracter }}</td>
            </tr>
            <tr>
                <th>Dieta</th>
                <td>{{ entity.dieta }}</td>
            </tr>
            <tr>
                <th>Observaciones</th>
                <td>{{ entity.observaciones }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('mascota') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('mascota_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
</ul>
{% endblock %}
